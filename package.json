{
  "name": "cedri",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "analyze": "yarn build:stats && webpack-bundle-analyzer build/bundle-stats.json -m server",
    "build": "rescripts build",
    "build:stats": "rescripts build --stats",
    "build-storybook": "build-storybook -s public",
    "dev": "rescripts start",
    "heroku-postbuild": "yarn build",
    "lint:fix": "standard --fix | snazzy",
    "start": "serve -s build",
    "storybook": "start-storybook -p 9009 -s public",
    "test": "rescripts test",
    "test:coverage": "rescripts test --coverage"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.17",
    "@fortawesome/free-solid-svg-icons": "5.8.1",
    "@fortawesome/react-fontawesome": "0.1.4",
    "@material-ui/core": "3.9.3",
    "@material-ui/icons": "3.0.2",
    "bootstrap": "4.3.1",
    "classnames": "2.2.6",
    "date-fns": "1.30.1",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-quill": "1.3.3",
    "react-router-dom": "5.0.0",
    "react-scripts": "2.1.8",
    "react-vanilla-form": "0.6.1",
    "reactstrap": "8.0.0",
    "serve": "11.0.0",
    "validator": "10.11.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.3",
    "@rescripts/cli": "0.0.10",
    "@rescripts/rescript-env": "0.0.10",
    "@storybook/addon-actions": "5.0.6",
    "@storybook/addon-links": "5.0.6",
    "@storybook/react": "5.0.6",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-react-require": "3.1.1",
    "chai": "4.2.0",
    "deep-freeze": "0.0.1",
    "eslint-config-standard": "12.0.0",
    "eslint-config-standard-react": "7.0.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-standard": "4.0.0",
    "react-hot-loader": "4.8.3",
    "react-test-renderer": "16.8.1",
    "snazzy": "8.0.0",
    "standard": "12.0.1",
    "webpack-bundle-analyzer": "3.2.0"
  },
  "rescripts": [
    "env"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "standard",
      "standard-react"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "fetch",
      "it",
      "expect"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
